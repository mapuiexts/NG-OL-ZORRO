{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@mapuiexts/ng-ol-zorro-antd": {
      "projectType": "library",
      "root": "projects/mapuiexts/ng-ol-zorro-antd",
      "sourceRoot": "projects/mapuiexts/ng-ol-zorro-antd/src",
      "prefix": "nolz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mapuiexts/ng-ol-zorro-antd/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mapuiexts/ng-ol-zorro-antd/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mapuiexts/ng-ol-zorro-antd/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/mapuiexts/ng-ol-zorro-antd/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "app-test": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/app-test",
      "sourceRoot": "projects/app-test/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-test",
            "index": "projects/app-test/src/index.html",
            "browser": "projects/app-test/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-test/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app-test/public"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "node_modules/ol/ol.css",
              "projects/app-test/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-test:build:production"
            },
            "development": {
              "buildTarget": "app-test:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-test/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app-test/public"
              }
            ],
            "styles": [
              "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "projects/app-test/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
